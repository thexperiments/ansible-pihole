vrrp_instance VI_1 {
        state {{ keepalived_role }}
        interface eth0
        virtual_router_id {{ keepalived_virtual_router_id }}
        priority {{ keepalived_priority }} 
        advert_int 1
        authentication {
              auth_type PASS
              auth_pass {{ keepalived_auth_pass }} 
        }
        virtual_ipaddress {
              {{ keepalived_virtual_ip }}
        }
}